    "babel-loader": "^8.0.5",
    "git-pre-push": "^0.0.5",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.3.1",
    "source-map-loader": "^0.2.4",
    "ts-node": "^8.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
